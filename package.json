  "scripts": {
    "compile": "node compile.js",
    "test": "node ./node_modules/mocha/bin/mocha.js \"Test/**/*.js\"",
    "ci-test": "mkdir -p test-results && node ./node_modules/mocha/bin/mocha.js \"Test/**/*.js\" --reporter mocha-junit-reporter --reporter-options mochaFile=test-results/results.xml,testsuitesTitle=\"Mocha Tests\"",
    "lint": "eslint . --ext .js --max-warnings=0",
    "ci-coverage": "mkdir -p test-results && nyc --reporter=cobertura node ./node_modules/mocha/bin/mocha.js \"Test/**/*.js\" --reporter mocha-junit-reporter --reporter-options mochaFile=test-results/results.xml,testsuitesTitle=\"Mocha Tests\"",
    "audit:ci": "npm audit --audit-level=high --json > audit.json || true"
  },
  "dependencies": {
    "ganache": "^7.9.0",
    "mocha": "^11.0.0",
    "solc": "^0.8.30",
    "web3": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "eslint": "^9.34.0",
